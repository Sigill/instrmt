{
  "compilerOptions": {
    "target": "es2021",
    "lib": [],
    "module": "es2022",
    "rootDir": "./",
    "moduleResolution": "node",
    "removeComments": true,
    "inlineSourceMap": true,
    "newLine": "lf",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "files": [
    "bootstrap.ts"
  ]
}
